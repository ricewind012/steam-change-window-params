{
	"$schema": "https://biomejs.dev/schemas/2.0.5/schema.json",
	"assist": {
		"actions": {
			"source": {
				"organizeImports": {
					"level": "on",
					"options": {
						"groups": [":PACKAGE:", ":BLANK_LINE:", ":PATH:"]
					}
				},
				"useSortedKeys": "on"
			}
		}
	},
	"files": {
		"includes": ["**", "!package.json", "!plugin.json"]
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	}
}
